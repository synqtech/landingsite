<!-- QGarage APP MODAL -->
<div class="modal fade" id="AppModalQGarage" tabindex="-1" role="dialog" aria-labelledby="AppModalQGarageLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header" style="background: url(<%= image_url 'blur/qgarage.jpg' %>)">
        <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
        <!-- <h4 class="modal-title" id="AppModalQDriveLabel">Modal title</h4> -->
        <%= image_tag "qgarage.png", :class=>"responsive" %>
      </div>
      <div class="modal-body">
        <div class="well"><h4>More Information Coming Soon</h4></div>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
        <button type="button" class="btn btn-primary">Save changes</button>
      </div>
    </div>
  </div>
</div>